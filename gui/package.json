{
	"name": "notemine-gui",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "./dev-relay.sh && vite dev",
		"dev:no-relay": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"relay:start": "./dev-relay.sh",
		"relay:stop": "./stop-relay.sh",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest --coverage",
		"lint": "eslint . && prettier --check .",
		"lint:fix": "eslint . --fix && prettier --write .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@testing-library/svelte": "^5.2.8",
		"@typescript-eslint/eslint-plugin": "^8.33.1",
		"@typescript-eslint/parser": "^8.33.1",
		"@vitest/ui": "^3.2.2",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.9.1",
		"jsdom": "^26.1.0",
		"postcss": "^8.5.4",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.0",
		"typescript": "^5.0.0",
		"vite": "^6.2.6",
		"vitest": "2.1.1"
	},
	"dependencies": {
		"@noble/hashes": "^1.8.0",
		"@notemine/core": "workspace:*",
		"@notemine/wrapper": "workspace:*",
		"@types/qrcode": "^1.5.5",
		"applesauce-core": "^1.2.0",
		"applesauce-relay": "^1.2.0",
		"applesauce-signers": "^1.2.0",
		"bech32": "^2.0.0",
		"dexie": "^4.0.11",
		"lucide-svelte": "^0.513.0",
		"minisearch": "^7.1.2",
		"nostr-tools": "2.7.2",
		"qrcode": "^1.5.4",
		"rxjs": "7.8.1",
		"tone": "^15.1.22"
	}
}
